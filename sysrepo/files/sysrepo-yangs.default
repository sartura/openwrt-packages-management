#!/bin/sh

if [ -x /bin/sysrepoctl ]; then
	sysrepoctl --install --yang=/etc/sysrepo/yang/ietf-netconf-acm.yang
	sysrepoctl --install --yang=/etc/sysrepo/yang/ietf-netconf-notifications.yang
	sysrepoctl --install --yang=/etc/sysrepo/yang/nc-notifications.yang
	sysrepoctl --install --yang=/etc/sysrepo/yang/notifications.yang
fi

exit 0

#!/bin/sh

if [ -x /bin/sysrepoctl ]; then
	sysrepoctl --install --yang=/etc/sysrepo/yang/ietf-netconf
	sysrepoctl --install --yang=/etc/sysrepo/yang/ietf-netconf-acm
	sysrepoctl --install --yang=/etc/sysrepo/yang/ietf-netconf-monitoring
	sysrepoctl --install --yang=/etc/sysrepo/yang/ietf-netconf-with-defaults
fi

exit 0
